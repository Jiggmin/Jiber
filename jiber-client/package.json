{
  "name": "jiber-client",
  "version": "0.1.11",
  "description": "Synced data store",
  "main": "dist/jiber-client.bundle.js",
  "module": "dist/es6/index.js",
  "types": "src/index.ts",
  "scripts": {
    "test": "jest src",
    "lint": "tslint --fix -p tsconfig.json",
    "prepare": "npm run build",
    "build": "tsc && rollup -c && uglifyjs -c -m -o dist/jiber-client.bundle.min.js dist/jiber-client.bundle.js",
    "compile": "tsc",
    "bundle": "rollup -c",
    "minify": "uglifyjs -c -m -o dist/jiber-client.bundle.min.js dist/jiber-client.bundle.js",
    "travis-ci": "npm run lint && npm run test -- --ci --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "author": "Jacob Grahn",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "jest": "^23.6.0",
    "rollup": "^1.0.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.2",
    "uglify-es": "^3.0.15",
    "wrtc": "^0.3.3"
  }
}
